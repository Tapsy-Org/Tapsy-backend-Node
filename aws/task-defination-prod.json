      "image": "IMAGE_PLACEHOLDER",
      "cpu": 0,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "name": "backend-prod-container-3000-tcp",
          "appProtocol": "http"
        }
      ],
      "command": [
        "sh",
        "-c",
        "node dist/server.js"
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/backend-prod-api",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "secrets": [
