{
    "family": "backend-prod",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "512",
    "memory": "2048",
    "executionRoleArn": "arn:aws:iam::029923774811:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::029923774811:role/ecsTaskExecutionRole",
    "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
    },
    "containerDefinitions": [
      {
        "name": "backend-prod-container",
        "image": "029923774811.dkr.ecr.us-west-1.amazonaws.com/tapsy/tapsy-backend-node:latest",
        "cpu": 0,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000,
            "protocol": "tcp",
            "name": "backend-dev-container-3000-tcp",
            "appProtocol": "http"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-create-group": "true",
            "awslogs-group": "/ecs/backend-prod",
            "awslogs-region": "us-west-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "volumes": [],
    "placementConstraints": []
  }
  